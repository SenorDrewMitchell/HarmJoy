<h1>Listing race_days</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Race date</th>
      <th>Race</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @race_days.each do |race_day| %>
      <tr>
        <td><%= race_day.name %></td>
        <td><%= race_day.race_date %></td>
        <td><%= race_day.race_id %></td>
        <td><%= link_to 'Show', race_day %></td>
        <td><%= link_to 'Edit', edit_race_day_path(race_day) %></td>
        <td><%= link_to 'Destroy', race_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Race day', new_race_day_path %>
